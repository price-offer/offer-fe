{
  "stylelint.validate": [
    "css",
    "scss",
    "typescriptreact",
    "typescript",
    "javascript",
    "javascriptreact"
  ],
  "stylelint.configFile": ".stylelintrc.json",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "cSpell.words": ["Pretendard", "stylelint", "unoptimized"]
}
